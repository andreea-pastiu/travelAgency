<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TextField?>
<VBox prefWidth="850" prefHeight="600" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.travelagency.controller.UserController">
    <children>
        <Label text="User page" />
        <HBox>
            <Label text="Username" />
            <TextField fx:id="usernameTextField" />
        </HBox>
        <HBox>
            <Label text="Password" />
            <TextField fx:id="passwordTextField" />
        </HBox>
        <HBox>
            <Button text="Login" onAction="#login"/>
            <Button text="Register" onAction="#register"/>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
