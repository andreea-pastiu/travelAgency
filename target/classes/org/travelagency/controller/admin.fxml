<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox prefWidth="850" prefHeight="600" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.travelagency.controller.AdminController">
    <children>
        <Label text="Admin page" />
        <Button fx:id="addDestinationButton" text="Add Destination" onAction="#addDestination" />
        <Button fx:id="viewDestinationsButton" text="View Destinations" onAction="#viewDestinations" />
        <Button fx:id="viewPackagesButton" text="View Packages" onAction="#viewPackages" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
